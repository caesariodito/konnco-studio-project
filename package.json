{
	"name": "konnco-studio-project",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate": "drizzle-kit generate:pg --config drizzle.config.ts",
		"db:seed": "node --loader esbuild-register/loader -r esbuild-register ./src/lib/server/db/seed.ts",
		"db:push": "drizzle-kit push:pg --config drizzle.config.ts",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts"
	},
	"type": "module",
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "^8.56.5",
		"@types/pg": "^8.11.2",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"autoprefixer": "^10.4.18",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.20.14",
		"esbuild-register": "^3.5.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lucia": "^3.1.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.1.5"
	},
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.3",
		"bits-ui": "^0.19.5",
		"clsx": "^2.1.0",
		"drizzle-orm": "^0.29.5",
		"lucide-svelte": "^0.350.0",
		"oslo": "^1.1.3",
		"pg": "^8.11.3",
		"postgres": "^3.4.3",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.2.0"
	}
}
